{
	"name": "vue-spa-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
		"lint": "eslint . --quiet",
		"lint:fix": "eslint . --fix",
		"stylelint": "stylelint \"**/*.{css,scss,vue}\" --quiet",
		"stylelint:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
		"typecheck": "vue-tsc --noEmit",
		"circular": "dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform \"./src/main.ts\"",
		"deadcode": "knip --exclude binaries,dependencies,unlisted",
		"check": "run-p typecheck lint stylelint circular deadcode",
		"fix": "run-p lint:fix stylelint:fix",
		"openapi-ts": "openapi-ts"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/vue-query": "^5.92.0",
		"@unhead/vue": "^2.0.19",
		"@vueuse/core": "^14.1.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-vue-next": "^0.555.0",
		"pinia": "^3.0.4",
		"reka-ui": "^2.6.1",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"vee-validate": "^4.15.1",
		"vue": "^3.5.24",
		"vue-router": "^4.6.3"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "^0.88.0",
		"@stylistic/stylelint-plugin": "^4.0.0",
		"@types/node": "^24.10.1",
		"@vitejs/plugin-vue": "^6.0.2",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/tsconfig": "^0.8.1",
		"dpdm": "^3.14.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-vue": "^10.6.2",
		"knip": "^5.71.0",
		"npm-run-all": "^4.1.5",
		"postcss-html": "^1.8.0",
		"stylelint": "^16.26.1",
		"stylelint-config-recess-order": "^7.4.0",
		"stylelint-config-recommended-vue": "^1.6.1",
		"stylelint-high-performance-animation": "^1.11.0",
		"stylelint-order": "^7.0.0",
		"stylelint-plugin-logical-css": "^1.2.3",
		"stylelint-plugin-use-baseline": "^1.1.3",
		"stylelint-use-nesting": "^6.0.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "npm:rolldown-vite@^7.2.8",
		"vue-tsc": "^3.1.5"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@^7.2.8"
	}
}
